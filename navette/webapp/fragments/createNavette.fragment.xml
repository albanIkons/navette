    <form:SimpleForm busy="{mainModel>/trasferBusy}" columnsXL="3" columnsL="2" columnsM="1" layout="ColumnLayout" labelSpanXL="3" labelSpanL="3" labelSpan="3">
        <form:toolbar>
            <Toolbar>
                <Title text="{i18n>createNavette}" />
            </Toolbar>
        </form:toolbar>
        <Label text="{i18n>navetteNr}"/>
        <Input id="navetteIdCreate" showValueHelp="true" valueHelpRequest=".onNavetteHelp" />
    </form:SimpleForm>

    <Table id="table" items="{items>/}" visible="true">
        <headerToolbar>
            <OverflowToolbar>
                <Title text="{i18n>itemText}" />
                <ToolbarSpacer />
                <Button text="{i18n>addWipOut}" icon="sap-icon://sys-add" press="onAddItem" />
            </OverflowToolbar>
        </headerToolbar>
        <columns>
            <Column minScreenWidth="Tablet" demandPopin="true">
                <Text text="{i18n>wipout}" wrapping="" />
            </Column>
            <Column minScreenWidth="Tablet" demandPopin="true">
                <Text text="{i18n>ordine}" wrapping="false" />
            </Column>
            <Column minScreenWidth="Tablet" demandPopin="true">
                <Text text="{i18n>materiale}" wrapping="false" />
            </Column>
            <Column minScreenWidth="Tablet" demandPopin="true">
                <Text text="{i18n>descrMateriale}" wrapping="false" />
            </Column>
            <Column minScreenWidth="Tablet" demandPopin="true">
                <Text text="Icona" wrapping="false" />
            </Column>
            <Column minScreenWidth="Tablet" demandPopin="true">
                <Text text="{i18n>stato}" wrapping="false" />
            </Column>
            <Column minScreenWidth="Tablet" demandPopin="true">
                <Text text="{i18n>centroLavoro}" wrapping="false" />
            </Column>
            <Column minScreenWidth="Tablet" demandPopin="true">
                <Text text="{i18n>quantita}" wrapping="false" />
            </Column>
            <Column minScreenWidth="Tablet" demandPopin="true">
                <Text text="{i18n>um}" wrapping="false" />
            </Column>
            <Column minScreenWidth="Tablet" demandPopin="true">
                <Text text="{i18n>rimuovi}" wrapping="false" />
            </Column>
        </columns>
        <items>
            <ColumnListItem>
                <cells>
                    <Input id="wip-out" key="{items>WIP_OUT}" value="{items>WIP_OUT}" showValueHelp="true" valueHelpRequest=".onWipoutHelp" />
                    <ObjectIdentifier text="{items>AUFNR}" />
                    <ObjectIdentifier text="{items>MATNR}" />
                    <ObjectIdentifier text="{items>MAKTX}" />
                    <ObjectIdentifier text="{items>STATO_COLLAUDO}" />
                    <ObjectIdentifier text="{items>STATO_COLORE}" />
                    <ObjectIdentifier text="{items>ARBPL}" />
                    <ObjectIdentifier text="{items>MENGE}" />
                    <ObjectIdentifier text="{items>MEINS}" />
                    <Button icon="sap-icon://delete" press="onDeleteItem" type="Reject" />
                </cells>
            </ColumnListItem>
        </items>
    </Table>

</core:FragmentDefinition>