<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog id="msgDialog" title="Informazione su Ricevimento">
		<beginButton>
			<Button text="Ok" press=".onCloseDialogMessage"/>
		</beginButton>
			<content>
				<SelectList items="{informationMsg>/}">
					<core:Item key="{informationMsg>item}" text="{informationMsg>message}"/>
				</SelectList>
			</content>
	</Dialog>
</core:FragmentDefinition>